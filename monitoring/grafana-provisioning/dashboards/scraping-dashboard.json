{
  "id": null,
  "title": "Distributed Web Scraping System Monitor",
  "tags": ["scraping", "fastapi", "workers", "redis"],
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "API Request Rate",
      "type": "stat",
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "rate(api_requests_total[1m])",
          "legendFormat": "Requests/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 2,
      "title": "Queue Jobs Total",
      "type": "stat",
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
      "targets": [
        {
          "expr": "queue_jobs_total",
          "legendFormat": "Jobs Queued"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 3,
      "title": "Scraping Success Rate",
      "type": "stat",
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "rate(scrape_success_total[5m]) / rate(scrape_total[5m]) * 100",
          "legendFormat": "Success Rate %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 4,
      "title": "Database Records",
      "type": "stat",
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
      "targets": [
        {
          "expr": "pg_stat_user_tables_n_tup_ins{relname='page_results'}",
          "legendFormat": "Total Records"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 5,
      "title": "API Request Latency",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(api_request_latency_bucket[5m]))",
          "legendFormat": "95th Percentile"
        },
        {
          "expr": "histogram_quantile(0.50, rate(api_request_latency_bucket[5m]))",
          "legendFormat": "50th Percentile"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 6,
      "title": "Scraping Performance",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "rate(scrape_success_total[1m])",
          "legendFormat": "Successful Scrapes/min"
        },
        {
          "expr": "rate(scrape_fail_total[1m])",
          "legendFormat": "Failed Scrapes/min"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 7,
      "title": "System Resource Usage",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "process_resident_memory_bytes{job='api'} / 1024 / 1024",
          "legendFormat": "API Memory (MB)"
        },
        {
          "expr": "process_resident_memory_bytes{job='worker'} / 1024 / 1024",
          "legendFormat": "Worker Memory (MB)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "decbytes",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 8,
      "title": "Error Rate by Endpoint",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "targets": [
        {
          "expr": "rate(api_requests_total{status=~'4..|5..'}[5m])",
          "legendFormat": "{{path}} - {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {"mode": "palette-classic"}
        }
      }
    }
  ]
}